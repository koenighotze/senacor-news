{
  "name": "senacor-aktuelles",
  "version": "1.0.0",
  "description": "News server for senacor events",
  "main": "index.js",
  "scripts": {
    "start": "node src/app.js",
    "test": "lab -r console -o stdout -r html -o build/report/coverage.html -r lcov -o build/report/lcov.info",
    "lint": "node_modules/jshint/bin/jshint src/ test/",
    "docker.build": "docker build -t koenighotze/senacor-news .",
    "docker.run": "docker run --rm=true -p 8000:8000 koenighotze/senacor-news",
    "package": "pushd build/tmp && cp -rf ../../package.json ../../src/* . && npm i --production && tar czf ../senacor-news.tgz * && popd"
  },
  "author": "David Schmitz",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^0.22.0",
    "config": "^1.26.1",
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.1.1",
    "hapi-swagger": "^7.7.0",
    "inert": "^4.2.0",
    "joi": "^10.6.0",
    "vision": "^4.1.1",
    "wreck": "^12.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-string": "^1.3.0",
    "chai-things": "^0.2.0",
    "jshint": "^2.9.4",
    "jsonfile": "^3.0.0",
    "lab": "^13.0.2",
    "mocha": "^3.3.0",
    "request": "^2.81.0",
    "sinon": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:koenighotze/senacor-news.git"
  }
}
